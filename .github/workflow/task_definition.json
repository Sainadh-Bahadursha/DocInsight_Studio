{
  "family": "docinsight-studio-td",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::839685189558:role/ecsTaskExecutionRole",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "8192",
  "containerDefinitions": [
    {
      "name": "docinsight-studio-container",
      "image": "839685189558.dkr.ecr.ap-southeast-2.amazonaws.com/docinsight-studio-ecr-repository",
      "cpu": 1024,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "name": "docinsight-studio-container-8080-tcp",
          "appProtocol": "http"
        }
      ],
      "environment": [
        {
          "name": "ENV",
          "value": "production"
        }
      ],
      "secrets": [
        {
          "name": "docinsight_studio_aws_secrets",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-2:839685189558:secret:docinsight_studio_aws_secrets-vPmscf"
        }
        
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/docinsight-studio-td",
          "awslogs-region": "ap-southeast-2",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      }
    }
  ]
}
